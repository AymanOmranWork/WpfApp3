    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\fluent-validation-logo copy.png" />
    <None Remove="Data\fluent-validation-logo.png" />
    <None Remove="Data\Infinity.png" />
