  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Data\fluent-validation-logo copy.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Data\fluent-validation-logo.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="Data\Infinity.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
